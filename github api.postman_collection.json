		{
			"name": "creating an issue for a repo ",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "62302b19-b2f4-4f2d-a38a-c7501e5db5ba",
						"exec": [
							"var jsonData = pm.response.json();",
							"pm.test(\"check label\", function () {",
							"    pm.expect(jsonData.labels[0].name).to.eql(\"bug\");",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"auth": {
